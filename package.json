  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/node": "^18.11.9",
    "chokidar": "^3.5.3",
    "commander": "^9.4.1",
    "swagger-merger": "^1.5.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
